    "dotenv": "^14.3.2",
    "dotenv-cli": "^6.0.0",
    "express": "~4.16.1",
    "express-list-routes": "^1.1.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "random-string": "^0.2.0",
    "redis": "^3.0.0",
    "redis-commander": "^0.8.0",
    "response-time": "^2.3.2",
