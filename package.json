    "devstart": "nodemon ./bin/www",
    "test": "PORT=4001 NODE_ENV=test jest --silent --config ./config/jest.config.js",
    "prettier:check": "prettier --check . --config ./config/.prettierrc.json --ignore-path ./config/.prettierignore ",
